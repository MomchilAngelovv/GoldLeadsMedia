@model GoldLeadsMedia.Web.Models.ViewModels.AdministratorsInformationViewModel

<div class="btn-group" role="group" aria-label="Basic example">
    <a asp-controller="Administrators" asp-action="CreateOffer" class="btn btn-primary">Create offer</a>
    <a asp-controller="Administrators" asp-action="AssignLandingPagesToOffer" class="btn btn-primary">Assign landing pages to offers</a>
    <a asp-controller="Administrators" asp-action="RegisterAffiliate" class="btn btn-primary">Register affiliate</a>
    <a asp-controller="Administrators" asp-action="RegisterLandingPage" class="btn btn-primary">Register landing Page</a>
    <a asp-controller="Administrators" asp-action="RegisterBroker" class="btn btn-primary">Register broker</a>
</div>

<hr />
<h2 class="text-center">Developer errors</h2>
<hr />

<div class="row">
    <div class="list-group col-12">
        @foreach (var devError in Model.DeveloperErrors)
        {
            <a href="#" class="list-group-item list-group-item-action">[@devError.CreatedOn] @devError.Message</a>
        }
    </div>
</div>

